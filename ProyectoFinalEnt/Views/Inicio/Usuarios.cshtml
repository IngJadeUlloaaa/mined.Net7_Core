@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <title>Usuarios</title>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
    <link rel="stylesheet" href="~/css/usuarioStyles.css" />
    <link rel="stylesheet" href="https://unicons.iconscout.com/release/v4.0.8/css/line.css">
</head>

<body>

    <!-- nabvar -->
    <header class="parent-container-navmenu-home">
        <div class="interface-container-navmenu-home">
            <div class="fchild-container-navmenu-home">
                <ul class="fbox-container-navmenu-home">
                    <li class="title-navmenu-home" onclick="location.href='https://www.mined.gob.ni/estudiantes/'"><i class="uil uil-book-open"></i>Services</li>
                </ul>

                <ul class="sbox-container-navmenu-home">
                    <li class="title-navmenu-home" onclick="location.href='https://www.mined.gob.ni/estudiantes/'"><i class="uil uil-newspaper"></i>News</li>
                </ul>

            </div>
            <!-- img Mined -->
            <div class="schild-container-navmenu-home">
                <img src="~/media/logo-mined.png" alt="" class="img-mined-navmenu-home" />
            </div>

            <div class="tchild-container-navmenu-home">
                <ul class="tbox-container-navmenu-home">
                    <li class="title-navmenu-home" onclick="location.href='https://www.mined.gob.ni/estudiantes/'"><i class="uil uil-book-open"></i>Library</li>
                </ul>

                <ul class="ftbox-container-navmenu-home">
                    <li class="title-navmenu-home" onclick="location.href='https://www.mined.gob.ni/estudiantes/'"><i class="uil uil-book-reader"></i>Educational Areas</li>
                </ul>
            </div>
        </div>
    </header>
    <!-- formats -->
    <section class="parent-container-format-home">

        <article class="child-container-format-home">
            <div class="fbox-container-format-home">
                <div class="box-container-format-home">

                    <article class="padding-container">
                        <div class="img-container-format-home">
                            <img src="~/media/mainers.jpg" alt="" class="mimg-format-home" />
                        </div>
                        <div class="container">
                            <h2 class="title-box-format-home">Director</h2>
                            <a href="@Url.Action("Registrarse_D","Inicio")">Registrarse</a>
                        </div>
                    </article>
                </div>

                <div class="box-container-format-home">
                    <article class="padding2-container">
                        <div class="img-container-format-home">
                            <img src="~/media/trucker.jpg" alt="" class="timg-format-home" />
                        </div>
                        <div class="container">
                            <h2 class="title-box-format-home">Truckers</h2>
                            <a href="@Url.Action("Registrarse_C","Inicio")">Registrarse</a>
                        </div>
                    </article>
                </div>
                <button onclick="window.location.href='http://www.google.com'" class="btn-container-format-home">Read more</button>
            </div>
            <div class="sbox-container-format-home">
                <div class="container2">
                    <div id="carouselExampleIndicators" class="carousel slide">
                        <div class="carousel-indicators">
                            <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="0" class="active" aria-current="true" aria-label="Slide 1"></button>
                            <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="1" aria-label="Slide 2"></button>
                            <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="2" aria-label="Slide 3"></button>
                        </div>
                        <div class="carousel-inner">
                            <div class="carousel-item active">
                                <img src="~/media/img1.jpg" class="d-block w-100 img-format-home" alt="...">
                            </div>
                            <div class="carousel-item">
                                <img src="~/media/img2.jpg" class="d-block w-100 img-format-home" alt="...">
                            </div>
                            <div class="carousel-item">
                                <img src="~/media/img3.jpg" class="d-block w-100 img-format-home" alt="...">
                            </div>
                        </div>
                        <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide="prev">
                            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                            <span class="visually-hidden">Previous</span>
                        </button>
                        <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide="next">
                            <span class="carousel-control-next-icon" aria-hidden="true"></span>
                            <span class="visually-hidden">Next</span>
                        </button>
                    </div>
                </div>
            </div>
        </article>
    </section>
    <!-- footer -->
    <footer class="footer-container-home">
        <article class="box-footer-container-home">
            <div class="fcontainer-footer">
                <h2 class="title-footer-format-home">3000+</h2>
                <h2 class="title-footer-format-home">   Aided Schools</h2>
            </div>
            <div class="fcontainer-footer">
                <h2 class="title-footer-format-home">30000+</h2>
                <h2 class="title-footer-format-home">   Food Supplies</h2>
            </div>
        </article>
        <article class="box2-footer-container-home">
            <div class="scontainer-footer">
                <a href="" class="a-footer">
                    <i id="fc" class="uil uil-facebook"></i>
                </a>
                <a href="" class="a-footer">
                    <i id="tl" class="uil uil-telegram"></i>
                </a>
                <a href="" class="a-footer">
                    <i id="ws" class="uil uil-whatsapp-alt"></i>
                </a>
            </div>
        </article>
    </footer>
    <br>
    <br>
</body>
</html> 